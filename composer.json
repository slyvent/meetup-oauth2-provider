{
    "name": "slyvent/meetup-oauth2-provider",
    "description": "Meetup OAuth2 Client Provider for Leagues OAuth2-Client",
    "version": "0.2.0",
    "authors": [
        {
            "name": "Hao Luo",
            "email": "howlowck@gmail.com"
        },
        {
            "name": "Karl Marques",
            "email": "kmarques@slyvent.com"
        }
    ],
    "keywords": [
